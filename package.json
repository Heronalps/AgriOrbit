{
  "name": "ag-advisor-map",
  "version": "1.0.0",
  "description": "Agricultural advice application with interactive map",
  "private": true,
  "scripts": {
    "prepare": "poetry install",
    "backend": "poetry run python -m backend.app.main",
    "frontend": "pnpm --filter ag-advisor-frontend dev",
    "dev": "concurrently \"pnpm run backend\" \"pnpm run frontend\"",
    "test": "pnpm run test:backend && pnpm run test:frontend",
    "test:backend": "poetry run pytest",
    "test:frontend": "pnpm --filter ag-advisor-frontend test",
    "build": "pnpm --filter ag-advisor-frontend build",
    "lint": "pnpm --filter ag-advisor-frontend lint",
    "setup": "pnpm install"
  },
  "keywords": [
    "agriculture",
    "farming",
    "map",
    "advice",
    "fastapi",
    "vue"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.6.4",
  "dependencies": {
    "@danmarshall/deckgl-typings": "^4.9.28",
    "@deck.gl/core": "9.1.12",
    "@deck.gl/extensions": "9.1.12",
    "@deck.gl/geo-layers": "9.1.12",
    "@deck.gl/layers": "9.1.12",
    "@deck.gl/mesh-layers": "9.1.12",
    "@vuepic/vue-datepicker": "^11.0.2",
    "axios": "^1.9.0",
    "mapbox-gl": "^3.12.0",
    "marked": "^15.0.11",
    "pinia": "^3.0.2",
    "vue": "^3.5.14",
    "vue-draggable-resizable": "^3.0.0",
    "vue-eslint-parser": "^10.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/mapbox-gl": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}
