{
  "name": "ag-advisor-map",
  "version": "1.0.0",
  "description": "Agricultural advice application with interactive map",
  "private": true,
  "scripts": {
    "prepare": "poetry install",
    "backend:dev": "poetry run uvicorn backend.app.main:app --reload --port 8157",
    "backend:start": "poetry run python -m backend.app.main",
    "backend:format:black": "poetry run black backend",
    "backend:format:isort": "poetry run isort backend",
    "backend:format": "concurrently \"pnpm run backend:format:black\" \"pnpm run backend:format:isort\"",
    "backend:lint:flake8": "poetry run flake8 backend",
    "backend:lint:isort": "poetry run isort --check-only backend",
    "backend:lint": "concurrently \"pnpm run backend:lint:flake8\" \"pnpm run backend:lint:isort\"",
    "backend:test": "poetry run pytest",
    "frontend:dev": "vite frontend --config frontend/vite.config.ts",
    "frontend:build": "vue-tsc --project frontend/tsconfig.json --noEmit && vite build frontend --config frontend/vite.config.ts",
    "frontend:serve": "vite preview frontend --config frontend/vite.config.ts",
    "frontend:format": "prettier --write \"**/*.{js,ts,vue,html,css,json,md,yaml,cjs,mjs}\"",
    "frontend:lint": "eslint --config frontend/eslint.config.js --fix frontend/src/**/*.{js,ts,vue} frontend/*.{js,ts,cjs,mjs}",
    "frontend:test": "echo \"Frontend tests not yet configured. Run 'pnpm --filter ag-advisor-frontend test' if defined locally.\"",
    "dev": "concurrently \"pnpm run backend:dev\" \"pnpm run frontend:dev\"",
    "build": "pnpm run frontend:build",
    "format": "pnpm run frontend:format && pnpm run backend:format",
    "lint": "concurrently \"pnpm run frontend:lint\" \"pnpm run backend:lint\"",
    "test": "pnpm run backend:test && pnpm run frontend:test",
    "setup": "pnpm install"
  },
  "keywords": [
    "agriculture",
    "farming",
    "map",
    "advice",
    "fastapi",
    "vue"
  ],
  "author": "Michael, Brenan, Nathaniel, Eric, Dawson, Logan",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@danmarshall/deckgl-typings": "^4.9.28",
    "@deck.gl/core": "9.1.12",
    "@deck.gl/extensions": "9.1.12",
    "@deck.gl/geo-layers": "9.1.12",
    "@deck.gl/layers": "9.1.12",
    "@deck.gl/mesh-layers": "9.1.12",
    "@vuepic/vue-datepicker": "^11.0.2",
    "axios": "^1.9.0",
    "dompurify": "^3.2.5",
    "mapbox-gl": "^3.12.0",
    "marked": "^15.0.11",
    "pinia": "^3.0.2",
    "primeicons": "^7.0.0",
    "primevue": "3.48.1",
    "vue": "^3.5.14",
    "vue-draggable-resizable": "^3.0.0",
    "vue-eslint-parser": "^10.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tailwindcss/postcss": "^4.1.6",
    "@types/mapbox-gl": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}
